{
  "manifest_version": 2,

  "name": "Foam",
  "description": "This extension summarizes articles with information about keywords and sentiments",
  "version": "0.1",
  "background": {"scripts": ["background.js"]},
  "browser_action": {
    "default_icon": "icon.png",
    //"default_popup": "popup.html",
    "default_title": "Foam"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "http://0.0.0.0:5000/"
  ]
}
